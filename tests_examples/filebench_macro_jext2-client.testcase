[meta]
ident: filebench_macro_jext2-client
name: Filebench: MACRO Workloads - jext2 client
description: Filebench

[require]
tempdir: yes

[run]
script: scripts/filebench_multi.sh
testenv: scripts/environment/jext2.sh

[env]
MOUNT: $HOME/jext2/jext2_daemon.sh -client
JEXT2_PATH: $HOME/jext2/
UMOUNT: fusermount -u
PERSONALITIES: webserver fileserver varmail
SLEEP: 60

[pass]
condition: returncode
value: 0

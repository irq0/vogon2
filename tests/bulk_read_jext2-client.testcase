[meta]
name: JEXT2(client) read performance test 10G
description: Create fresh filesystem and sequentially read file to /dev/null. Use jext2 with client jvm

[require]
tempdir: yes

[run]
script: scripts/bulk_read_speed_generic.sh
testenv: scripts/environment/jext2.sh

[env]
FILESIZE: 10000
MOUNT: $HOME/jext2/jext2_daemon.sh -client
JEXT2_PATH: $HOME/jext2/
UMOUNT: fusermount -u
JAVA: true

[pass]
condition: returncode
value: 0

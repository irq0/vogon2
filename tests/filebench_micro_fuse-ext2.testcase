[meta]
ident: filebench_micro_fuse-ext2
name: Filebench: Sequential Write - One Thread - fuse ext2
description: Filebench 

[require]
tempdir: yes

[run]
script: scripts/filebench_multi.sh
testenv: scripts/environment/fuse-ext2.sh

[env]
MOUNT: $HOME/fuse-ext2/current-version/fuse-ext2/fuse-ext2 
MOUNT_POSTOPTS: -o rw+
BINARY: $HOME/fuse-ext2/current-version/fuse-ext2/fuse-ext2 
UMOUNT: fusermount -u
PERSONALITIES: create_bs4k_count1048576
SLEEP: 60

[pass]
condition: returncode
value: 0

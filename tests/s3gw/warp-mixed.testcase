[test]
name = "Warp: Mixed Workload 100 Obj 10 Connections"
description = "Warp"
requirements = ["tmpdir"]

[run]
test_script = "scripts/run-warp.sh"
testenv = "scripts/environment/s3gw.sh"

[env]
WARP_TEST = "get --host=localhost:7480 --access-key=test --secret-key=test --objects=100 --concurrent=10"

[pass]
condition = "returncode"
value = 0

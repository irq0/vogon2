[meta]
ident: filebench_micro_jext2-server
name: Filebench: Sequential Write - One Thread - jext2 server
description: Filebench 

[require]
tempdir: yes

[run]
script: scripts/filebench_multi.sh
testenv: scripts/environment/jext2.sh

[env]
MOUNT: $HOME/jext2/jext2_daemon.sh -server
JEXT2_PATH: $HOME/jext2/
UMOUNT: fusermount -u
PERSONALITIES: create_bs4k_count1048576
SLEEP: 60

[pass]
condition: returncode
value: 0
